{
  "source": {
    "include": ["js", "package.json", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "template": "./node_modules/minami"
  },
  "templates": {
    "default": {
      "includeDate": false
    }
  }
}
